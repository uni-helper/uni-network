{
  "name": "@uni-helper/uni-network",
  "version": "0.12.6",
  "description": "为 uni-app 打造的基于 Promise 的 HTTP 客户端",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "request",
    "network",
    "upload",
    "uploadFile",
    "download",
    "downloadFile"
  ],
  "homepage": "https://github.com/uni-helper/uni-network#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-network/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-network.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "pnpm run build --watch",
    "lint": "run-p lint:eslint lint:markdownlint check:types test:coverage",
    "lint:eslint": "eslint ./src --ext=.js,.cjs,.mjs,.ts,.cts,.mts --fix --cache --no-error-on-unmatched-pattern --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "release": "release-it",
    "prepare": "is-ci || husky install",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dcloudio/types": "^3.2.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/qs": "^6.9.7",
    "@types/statuses": "^2.0.0",
    "lodash.merge": "^4.6.2",
    "qs": "^6.11.0",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@commitlint/prompt": "^17.4.1",
    "@modyqyw/fabric": "^5.16.0",
    "@modyqyw/utils": "^3.2.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.11.18",
    "@types/ungap__url-search-params": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@ungap/url-search-params": "^0.2.2",
    "@vitest/coverage-c8": "^0.27.0",
    "@vitest/ui": "^0.27.0",
    "c8": "^7.12.0",
    "commitizen": "^4.2.6",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "release-it": "^15.6.0",
    "rollup": "^3.9.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.2",
    "type-fest": "^3.5.1",
    "typescript": "^4.9.4",
    "vitest": "^0.27.0"
  },
  "packageManager": "pnpm@7.24.3",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
