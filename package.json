{
  "name": "@uni-helper/uni-network",
  "version": "0.17.0",
  "description": "为 uni-app 打造的基于 Promise 的 HTTP 客户端",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "request",
    "network",
    "upload",
    "uploadFile",
    "download",
    "downloadFile"
  ],
  "homepage": "https://github.com/uni-helper/uni-network#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-network/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-network.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./composables": {
      "import": {
        "types": "./dist/composables.d.mts",
        "default": "./dist/composables.mjs"
      },
      "require": {
        "types": "./dist/composables.d.cts",
        "default": "./dist/composables.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "taze -f",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\"",
    "lint:eslint": "eslint ./src --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "release": "pnpm install && pnpm run lint && pnpm run check:types && bumpp",
    "prepublishOnly": "pnpm run build",
    "prepare": "is-ci || simple-git-hooks",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dcloudio/types": "^3.4.3",
    "@types/lodash.merge": "^4.6.8",
    "@types/statuses": "^2.0.3",
    "@vueuse/core": "^9.13.0",
    "lodash.merge": "^4.6.2",
    "query-string": "^8.1.0",
    "statuses": "^2.0.1",
    "vue-demi": "^0.14.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/prompt": "^18.2.0",
    "@modyqyw/fabric": "^9.0.6",
    "@modyqyw/utils": "^8.0.0",
    "@tsconfig/node18": "^18.2.2",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.8.10",
    "@types/ungap__url-search-params": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@ungap/url-search-params": "^0.2.2",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "bumpp": "^9.2.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.0.2",
    "markdownlint-cli": "^0.38.0",
    "prettier": "^3.0.3",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.0",
    "tsx": "^3.14.0",
    "type-fest": "^4.6.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6",
    "vue": "^3.3.7"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0",
    "vue": "^2.6.0 || ^2.7.0 || ^3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
