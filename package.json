{
  "name": "@uni-helper/uni-network",
  "version": "0.15.0",
  "description": "为 uni-app 打造的基于 Promise 的 HTTP 客户端",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "request",
    "network",
    "upload",
    "uploadFile",
    "download",
    "downloadFile"
  ],
  "homepage": "https://github.com/uni-helper/uni-network#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-network/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-network.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./composables": {
      "types": "./dist/composables.d.ts",
      "import": "./dist/composables.mjs",
      "require": "./dist/composables.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "taze -f",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\"",
    "lint:eslint": "eslint ./src --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "release": "pnpm install && pnpm run lint && bumpp",
    "prepublishOnly": "pnpm run build",
    "prepare": "is-ci || simple-git-hooks",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dcloudio/types": "^3.3.3",
    "@types/lodash.merge": "^4.6.7",
    "@types/statuses": "^2.0.1",
    "@vueuse/core": "^9.13.0",
    "lodash.merge": "^4.6.2",
    "query-string": "^8.1.0",
    "statuses": "^2.0.1",
    "vue-demi": "^0.14.5"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/prompt": "^17.6.6",
    "@modyqyw/fabric": "^8.3.2",
    "@modyqyw/utils": "^7.1.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.16.19",
    "@types/ungap__url-search-params": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@ungap/url-search-params": "^0.2.2",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "bumpp": "^9.1.1",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.3",
    "markdownlint-cli": "^0.35.0",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.11.2",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "type-fest": "^3.12.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.2",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0",
    "vue": "^2.6.0 || ^2.7.0 || ^3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.6.5",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
