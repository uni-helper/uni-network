{
  "name": "@uni-helper/uni-network",
  "version": "0.21.3",
  "description": "为 uni-app 打造的基于 Promise 的 HTTP 客户端",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "request",
    "network",
    "upload",
    "uploadFile",
    "download",
    "downloadFile"
  ],
  "homepage": "https://github.com/uni-helper/uni-network#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-network/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-network.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./composables": {
      "import": {
        "types": "./dist/composables.d.ts",
        "default": "./dist/composables.js"
      },
      "require": {
        "types": "./dist/composables.d.cts",
        "default": "./dist/composables.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts",
        "./dist/composables.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "dev": "tsdown --watch"
  },
  "dependencies": {
    "@dcloudio/types": "^3.4.16",
    "@types/lodash.merge": "^4.6.9",
    "fast-querystring": "^1.1.2",
    "lodash.merge": "^4.6.2",
    "statuses-es": "^2.0.6",
    "vue-demi": "^0.14.10"
  },
  "devDependencies": {
    "@types/ungap__url-search-params": "^0.1.2",
    "@ungap/url-search-params": "^0.2.2",
    "@vueuse/core": "^13.5.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.18"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0",
    "@vueuse/core": "^9.0.0 || ^10.0.0 || ^11.0.0 || ^12.0.0 || ^13.0.0",
    "vue": "^2.6.0 || ^2.7.0 || ^3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    },
    "@vueuse/core": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
